				<div class="row">
					<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding">

						<div class="row">
							<table id="transcript-history-details-table" class="transcript-history-details-table table table-hover col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding" >
								<thead>
									<tr>
										<th>Date and Time</th>
										<th>Connection ID</th>
										<th>Status</th>
										<th>Recieved By</th>
										 {if $attributes.hasPhone }
										   <th id='phno_head' class='due_wd'>Phone</th>
										   <th id='obHead' class='obID' style='visiblity:hidden'></th>
										 {/if}
									</tr>
								</thead>

								<tbody id="transcript-table-tbody" class="transcript-table-tbody">
									{foreach $interactionHistory in $attributes.interactionHistories}
										<tr>
											<td>{$interactionHistory.dateString}</td>
											<td>{$interactionHistory.connectionId}</td>
											<td>{$interactionHistory.action}</td>
											<td>{$interactionHistory.AgentLogin}</td>
											{if not not($interactionHistory.phoneNumber)}
												{if $interactionHistory.outboundConnectionID == 'NA' and $interactionHistory.phoneNumber == 'NA' }
														<td class='due_wd' title='NA'>{$interactionHistory.phoneNumber}</td>
														<td class='obID'><div id='d_clip_button' class='clip_button fa fa-files-o' data-clipboard-text='NA' title='Click to copy.' style='float: right;cursor: pointer;margin-top:1px;visibility:hidden'></div></td>
												{else}
														<td class='due_wd' title='{$interactionHistory.outboundConnectionID}'>{$interactionHistory.phoneNumber}</td>
														<td class='obID'><div id='d_clip_button' class='clip_button fa fa-files-o' data-clipboard-text='{$interactionHistory.outboundConnectionID}' title='Click to copy.' style='float: right;cursor: pointer;margin-top:1px;'></div></td>
												{/if}
											    
											{/if}
										</tr>
										{ifempty}
											<td colspan="4">No History</td>
									{/foreach}
								</tbody>

							</table>
						</div>

					</div>
				</div>

           </div>
