/**
 * Interaction PopOver Content
 * @param attributes MainContainer Details
 */
{template .interactionPopOverContent}
				<!-- Interaction PopOver Content Starts -->
				<!-- {$attributes.test} -->
				<table class="interaction-pop-over-table">
					<thead>
						<tr><td colspan="2">Interaction Type</td><td>Completed</td><td>In-Queue</td><tr>
					</thead>
					<tbody>
						{foreach $interactionOptions in $attributes.interactionsOptionsArray}
								<tr><td class="color-code"><div style="background-color: {$interactionOptions.interactionTypeDetails.color};">&nbsp;</div></td><td><div>{$interactionOptions.interactionTypeDetails.name}</div></td><td><div>{$interactionOptions.completedTranscriptLength}</div></td><td class="last"><div>{$interactionOptions.inqueueTranscriptLength}</div></td></tr>
							{ifempty}
								<tr>
									<td class="color-code"><div style="background-color: #000000;">&nbsp;</div></td>
									<td colspan="3"><div>No interaction</div></td>
								</tr>
						{/foreach}
					</tbody>
				</table>
				<!-- Interaction PopOver Content Ends -->
{/template}


