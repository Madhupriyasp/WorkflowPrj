/**
 * Transcript Details
 * @param attributes chat history list
 */
{template .transcriptFullDetailChat}
			<!-- Transcript Full Detail Chat Starts -->
			<!-- {$attributes.test} -->
			
			
			


	<div id="transcript-full-details" class="transcript-full-details">
		<div class="row">
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding">
				<div class="row">
					<table id="transcript-full-details-table" class="transcript-full-details-table col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding">
						<thead>
							<tr id="transcript-details-domain-name-tr" class="transcript-details-domain-name-tr">
								<th colspan="2">{$attributes.messageDetails.domainName}</th>
							</tr>
							<tr id="from-to-details-tr" class="from-to-details-tr">
								<th colspan="3">
									<table id="transcript-full-details-user-details" class="transcript-full-details-user-details">
										<tr>
											<td rowspan="2">
												<img src= "{$attributes.messageDetails.userImage}"  id="transcript-full-details-img" class="transcript-full-details-img" />
											</td>
											<td>
											    {if not not($attributes.messageDetails.name)}
													<p>{$attributes.messageDetails.name}</p>
													<span style="font-size:14px; color:#999999; margin-top: 2px;">{$attributes.messageDetails.from}</span>
											    {else}
											    		{$attributes.messageDetails.from}
												{/if}
											</td>
										</tr>
									</table>
								</th>
							</tr>
						</thead>
						<tbody id="transcript-full-chat-details-tbody" class="transcript-full-chat-details-tbody">
									<tr>
										<td class="chatheader">User</td>
										<td class="chatheader">Chat</td>
										<td class="chatheader">Dates and Time</td>
									</tr>
									{if isNonnull($attributes.metaData)}
									<tr>
									    <td></td>
									    <td class="chatmsg">
									    		<table>
									                   {foreach $metaData in $attributes.metaData}
									                     <tr>
									                     	<td>{$metaData.key}</td>
									                     	<td>: {$metaData.value}</td>
									                     </tr>
									                   {/foreach}
									    		</table>
									    </td>
									    <td></td>
									</tr>
									{/if}
											{foreach $message in $attributes.messages}
													{if $message.user == 'Prechatsurvey' or $message.user == 'Offlineform'}
													     {if not not($message.chat) and $message.chat.length > 0}
													     	<tr>
																<td class="chatuser"><p>{$message.user}</p></td>
																<td>
																	<table>
														                   {foreach $formData in $message.chat}
														                     <tr>
														                     	<td>{$formData.key}</td>
														                     	<td>: {$formData.value}</td>
														                     </tr>
														                   {/foreach}
														    		</table>
														    	</td>
														    	<td><p><span>{$message.time}</span></p></td>	
													    	</tr>
												    	{/if}
												    {elseif $message.user == 'feedback'}
												    	<tr> 
												    		<td style="font-size:13px; color:#64696d; font-weight:bold; vertical-align: top;padding-left:9px; padding-top: 14px; padding-right: 20px;">Visitor</td>
												    		<td class="visitor-feedback" style="width: 70%;">
												    		 	{foreach $feedBackData in $message.chat}
														                  {if $feedBackData.key == 'rating' and not not($feedBackData.value) }
														                  			<p style="font-size:14px;  padding: 2px 0px 0px; margin: 0px;"><label>Ratings:</label><ul class="ratings">
																	               {for $i in range(5)}
																	               			{if $i <  $feedBackData.value}
																	               			   <li class="rated hover"></li>
																	               			{else}
																	               				<li></li>
																	               			{/if}
																	               {/for}
																	               </ul> </p>
														                  {/if}
														                  {if $feedBackData.key == 'feedback' and not not($feedBackData.value) }
														                    <p style="font-size:14px;  padding: 5px 5px 0px; margin: 0px;"> <label>Feedback:</label> <span>{$feedBackData.value}</span> </p>
														                  {/if}
														       	{/foreach}
												    		</td> 
												    		<td><p><span>{$message.time}</span></p></td>
												    	</tr>
												    {else}	
												    	<tr>
															<td class="chatuser"><p>{$message.user}</p></td>
															{if not $message.user and not $message.time}
																<td class="chatmsg"><p><b><center>{$message.chat}</center></b></p></td>
															{else}
															   		<td class="chatmsg">
																   		<p>
																	   		{if strContains($message.chat, 'Visitor navigated to') }
																	   			{$message.chat}
																	   		{else}
																	   			{$message.chat}
																	   		{/if}
																   		</p>
															   		</td>
															{/if}
															<td><p><span>{$message.time}</span></p></td>
														</tr>	
													{/if}
											{/foreach}									
								</tbody>

							</table>
						</div>

					</div>
				</div>
           	</div>
           	<!-- Transcript Full Detail Chat Ends -->
{/template}